{
  "name": "tenki-forecast-client",
  "version": "0.1.1",
  "description": "SPA for tenki-forecast app",
  "author": "David Antunes <dvdantunes@gmail.com>",
  "homepage": "https://github.com/dvdantunes/tenki-forecast",
  "main": "pages/index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "dev" : "next",
    "build" : "next build",
    "start": "next start",
    "start:debug": "cross-env DEBUG=tenki-forecast:* yarn start",
    "lint": "esw *.js . config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --recursive './*.js'",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --recursive './*.js'",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dvdantunes/tenki-forecast.git"
  },
  "keywords": [
    "weather",
    "tenki",
    "weather-forecast",
    "tenki-forecast",
    "express",
    "node",
    "node.js",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API"
  ],
  "dependencies": {
    "next": "7.0.2",
    "@zeit/next-css" : "1.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "google-map-react": "^1.1.1",
    "@sentry/node": "4.3.2"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "mocha": "3.5.0",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "license": "GPL-3.0"
}
