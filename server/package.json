{
  "name": "tenki-forecast-server",
  "version": "0.1",
  "description": "RESTful api server for tenki-forecast app",
  "author": "David Antunes <dvdantunes@gmail.com>",
  "homepage": "https://github.com/dvdantunes/tenki-forecast",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "node index.js; mkdir -p logs",
    "start:debug": "cross-env DEBUG=tenki-forecast-server:* yarn start",
    "lint": "esw *.js . config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --recursive './*.js'",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --recursive './*.js'",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dvdantunes/tenki-forecast.git"
  },
  "keywords": [
    "weather",
    "tenki",
    "weather-forecast",
    "tenki-forecast",
    "express",
    "node",
    "node.js",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API"
  ],
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "4.16.3",
    "express-validation": "1.0.2",
    "express-winston": "2.5.0",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "joi": "10.6.0",
    "method-override": "^2.3.10",
    "request" : "^2.88.0",
    "redis" : "^2.8.0",
    "morgan": "1.9.0",
    "winston": "2.4.1",
    "@sentry/node": "4.3.2",
    "dark-sky" : "1.1.4"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "mocha": "3.5.0",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2"
  },
  "license": "GPL-3.0"
}
